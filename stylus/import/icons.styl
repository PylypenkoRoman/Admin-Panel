@charset "UTF-8"

@font-face
  font-family: "jujus-admin"
  src:url("../fonts/jujus-admin.eot")
  src:url("../fonts/jujus-admin.eot?#iefix") format("embedded-opentype"),
  url("../fonts/jujus-admin.woff") format("woff"),
  url("../fonts/jujus-admin.ttf") format("truetype"),
  url("../fonts/jujus-admin.svg#jujus-admin") format("svg")
  font-weight: normal
  font-style: normal



[data-icon]:before
  font-family: "jujus-admin" !important
  content: attr(data-icon)
  font-style: normal !important
  font-weight: normal !important
  font-variant: normal !important
  text-transform: none !important
  speak: none
  line-height: 1
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale


[class^="icon-"]:before,
[class*=" icon-"]:before
  font-family: "jujus-admin" !important
  font-style: normal !important
  font-weight: normal !important
  font-variant: normal !important
  text-transform: none !important
  speak: none
  line-height: 1
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale


